#include <stdio.h>
#include <string.h>

int main() {
    char text[15];
    int num1 = -42;
    unsigned int num2 = 424242;
    strcpy(text, "example");

    printf("[num1] Dec: %d, Hex: %x, Unsigned: %u\n", num1, num1, num1);
    printf("[num2] Dec: %d, Hex: %x, Unsigned: %u\n", num2, num2, num2);
    printf("[field width on num2] 3: '%3u', 10: '%10u', '%08u'\n", num2, num2, num2);
    printf("[text] %s  Address %p\n", text, (void*)text);

    printf("variable num1 is at address: %p\n", (void*)&num1);
    
    return 0;
} 
